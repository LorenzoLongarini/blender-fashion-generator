{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Blender Script",
      "type": "shell",
      "command": "/Applications/Blender.app/Contents/MacOS/Blender",
      "args": [
        "--background",
        "--python",
        "${workspaceFolder}/generate_datasets.py"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ]
}
